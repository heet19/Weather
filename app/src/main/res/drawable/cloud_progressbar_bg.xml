<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="#354346"/>
            <corners
                android:radius="50dp"/>
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <gradient
                    android:startColor="#D4E7EA"
                    android:centerColor="#B2C2C4"
                    android:endColor="#909D9E"
                    android:angle="0"/>
                <corners
                    android:radius="50dp"/>
            </shape>
        </clip>
    </item>
</layer-list>